<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:id="@+id/horzprog1">
    <shape android:shape="rectangle" android:useLevel="false" android:thickness="8dp" >
      <solid android:color="#FFFFFF"></solid>
        <corners android:radius="4dp"></corners>

    </shape>
</item>
    <item android:id="@+id/horzprog2">
        <clip>
            <shape android:shape="rectangle" android:useLevel="true" android:thickness="10dp">
                <solid android:color="#0D3564"></solid>
                <corners android:radius="4dp"></corners>
            </shape>
        </clip>
    </item>

</layer-list>